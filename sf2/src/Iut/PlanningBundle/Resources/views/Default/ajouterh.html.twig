{% extends "PlanningBundle::layoutheures.html.twig" %}
{% block body %}
<h3>Formulaire d'une heure</h3>

{% for notice in app.session.flashbag.get('info') %}
	<p class="alert alert-info">{{ notice }}</p>
{% endfor %}

<form method="post" novalidate="novalidate">
  {{ form_widget(form) }}
  <input type="submit" class="btn btn-primary" />
</form>
{% endblock %}